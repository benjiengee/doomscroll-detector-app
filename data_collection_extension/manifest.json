{
  "manifest_version": 3,
  "name": "YouTube Watch Data Collector",
  "version": "1.0",
  "description": "Tracks Title and Prompts user for label",
  "permissions": ["tabs", "storage", "scripting"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "host_permissions": ["*://*.youtube.com/*"]
}
